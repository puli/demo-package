        "webmozart/expression": {
            "install-path": "vendor/webmozart/expression",
            "installer": "composer"
        },
        "webmozart/glob": {
            "install-path": "vendor/webmozart/glob",
            "installer": "composer"
        },
        "webmozart/json": {
            "install-path": "vendor/webmozart/json",
            "installer": "composer",
            "env": "dev"
        },
        "webmozart/path-util": {
            "install-path": "vendor/webmozart/path-util",
            "installer": "composer"
        }
